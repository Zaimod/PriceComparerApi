@page "/category"
@model CarParts.Pages.CategoryModel
@{
    ViewData["Title"] = "Categories";
}

<link rel="stylesheet" href="https://cdn.korzh.com/ed/1.2.6/easydata.min.css" />

<div id="EasyDataContainer"></div>

@section Scripts {
    <script src="https://cdn.korzh.com/ed/1.2.6/easydata.min.js" type="text/javascript"></script>
    <script>
        window.addEventListener('load', function () {
            new easydata.crud.EasyDataViewDispatcher({
                rootEntity: "Category"
            }).run()
        });
    </script>
}